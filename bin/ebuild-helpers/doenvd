if [[ $# -lt 1 ]] ; then
	source "${PORTAGE_BIN_PATH}"/isolated-functions.sh || exit 1
	__helpers_die "${0##*/}: at least one argument needed"
	exit 1
fi

_E_INSDESTTREE_='/etc/env.d/' exec doins "$@"
